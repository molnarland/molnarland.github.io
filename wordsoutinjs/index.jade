doctype html
html
	head
		script(type="text/javascript" src="wordsoutin.min.js")
		script(type="text/javascript" src="jscolor.min.js")
		script(type="text/javascript" src="jquery-2.1.4.min.js")
		link(href="style.css" type="text/css" rel="stylesheet")
		title wordsOutInJS 
		meta(charset="utf8")
		meta(name="description" content="Words Out In is an simple free TypeScript so words or word write as you type. You can add delay in between characters and wait in between words and words' colors. And if you does not want that typing at last word then just you can add the false value to last parameter.")
		meta(name="keywords" content="type,script,js,javascript,words out in,simulate type,vanilla javascript, vanilla js")
		meta(name="robots" content="index, follow")
		meta(name="Robots" content="All")
		meta(name="author" content="rrmolnar@gmail.com")
		meta(name="generator" content="Molnar Roland")
	body
		#description
			h1#project-name
				a(href="https://github.com/molnarland/wordsOutInJS" target="_blank") wordsOutInJS
			h3#proposer wordsOutInJS is a simulates typing with vanilla JavaScript.
				br 
				u No jQuery
				|  or others. Really easy to use, few lines. 
				br
				| Working in mobile and desktop.
		#download
			a(href="https://github.com/molnarland/wordsOutInJS/archive/master.zip").btn Download
			a(href="https://github.com/molnarland/wordsOutInJS#readme" target="_blank").btn Readme
		.settings
			#words
				input(type="text" placeholder="Words with '|' between")
			#delay
				div
					p Start delay: 
					input(type="range" min="1" max="2000" value="600")
					p 600
				div
					p End delay: 
					input(type="range" min="1" max="2000" value="900")
					p 900
			#wait
				div
					p Start wait: 
					input(type="range" min="1" max="2000" value="150")
					p 150
				div
					p End wait: 
					input(type="range" min="1" max="2000" value="500")
					p 500
			#color
				div
					label(for="firstcolor") Start color:
					input(type="text" class="jscolor {hash:true}" value="0CFF06")#firstcolor
				div
					label(for="secondcolor") End color:
					input(type="text" class="jscolor {hash:true}" value="040AAB")#secondcolor
			#stop
				label(for="stopCheck") Stop in the end:
				input(type="checkbox")#stopCheck
			#ok
				input(type="button" value="Start")#startbtn
				input(type="button" value="Stop")#stopbtn

		#woi
			h2#woitxt

		p#inspirate


